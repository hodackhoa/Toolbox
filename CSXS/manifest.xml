<?xml version='1.0' encoding='UTF-8'?>
<ExtensionManifest ExtensionBundleId="com.sam.toolbox" ExtensionBundleVersion="1.0.0" Version="7.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ExtensionList>
    <Extension Id="com.sam.toolbox.panel" Version="1.0.0" />
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="PHSP" Version="18" />
      <Host Name="PHXS" Version="18" />
    </HostList>
    <LocaleList>
      <Locale Code="All" />
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="7.0" />
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.sam.toolbox.panel">
      <DispatchInfo>
        <Resources>
          <MainPath>./client/index.html</MainPath>
          <ScriptPath>./host/index.jsx</ScriptPath>
          <CEFCommandLine />
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>Toolbox</Menu>
          <Geometry>
            <Size>
				<Height>350</Height>
				<Width>230</Width>
            </Size>
            <MinSize>
	            <Height>80</Height>
	            <Width>110</Width>
            </MinSize>
            <MaxSize>
	            <Height>1200</Height>
	            <Width>1920</Width>
            </MaxSize>
          </Geometry>
          <Icons>
          	<Icon Type="Normal">./client/LogoSAM.png</Icon>
          	<Icon Type="RollOver">./client/LogoSAM.png</Icon>
 			<Icon Type="DarkNormal">./client/LogoSAM.png</Icon>
 			<Icon Type="DarkRollOver">./client/LogoSAM.png</Icon>
          </Icons>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>